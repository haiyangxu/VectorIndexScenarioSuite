{
  "AppSettings": {
    "accountEndpoint": "https://your-account-endpoint.documents.azure.com:443/",
    "authKeyOrResourceTokenCredential": "your-auth-key-or-resource-token",
    "cosmosDatabaseId": "vector-benchmarking",
    "cosmosContainerId": "msmarco-100M-10",
    "cosmosContainerRUInitial": "60000", // will use default RU if it's 0 othereise will override with the value provided
    "cosmosContainerRUFinal": "100000", // Final RU for the container update to leverage the full RU
    "dataFilesBasePath": "C:\\src\\big-ann-benchmarks\\data\\msmarco_websearch",
    "errorLogBasePath": "C:\\Logs",
    "scenario": {
      "name": "MS-Marco-embedding-only", //"wiki-cohere-english-embedding-only",
      /* Not applicable to streaming scenario but single shot ingestion */
      "sliceCount": "100000000",
      "runIngestion": true,

      "runQuery": false,
      "numQueries": 5000,
      /* Number of parallel batches reading dataset file and queing for ingestion */
      "numBulkIngestionBatchCount": 400,
      "warmup": {
        "enabled": true,
        "numWarmupQueries": 100
      },
      "computeRecall": true,
      "computeLatencyAndRUStats": true,
      "kValues": [ 10 ],
      "streaming": {
        "startOperationId": 1,
        "totalVectors": 2300000
      }
    },
    "waitForUserInputBeforeExit": true
  }
}

